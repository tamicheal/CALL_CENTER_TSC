/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP (1000) [BusinessEntityID]
      ,[Title]
      ,[FirstName]
      ,[MiddleName]
      ,[LastName]
      ,[Suffix]
      ,[JobTitle]
      ,[Department]
      ,[GroupName]
      ,[StartDate]
  FROM [AdventureWorks2016_EXT].[HumanResources].[vEmployeeDepartment]
  WHERE JobTitle LIKE '%Manager%'
  AND Department = 'Sales';
